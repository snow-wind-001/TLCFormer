{
  "train_batch_size": 32,
  "train_micro_batch_size_per_gpu": 8,
  "gradient_accumulation_steps": 1,
  "steps_per_print": 100,
  "optimizer": {
    "type": "AdamW",
    "params": {
      "lr": 0.001,
      "betas": [
        0.9,
        0.999
      ],
      "eps": 1e-08,
      "weight_decay": 0.0001
    }
  },
  "scheduler": {
    "type": "WarmupLR",
    "params": {
      "warmup_min_lr": 0,
      "warmup_max_lr": 0.001,
      "warmup_num_steps": 1000
    }
  },
  "fp16": {
    "enabled": false
  },
  "wall_clock_breakdown": false,
  "tensorboard": {
    "enabled": true,
    "output_path": "./runs/rgbt_tiny_deepspeed",
    "job_name": "osformer_deepspeed_basic"
  }
}